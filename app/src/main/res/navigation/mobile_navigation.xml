<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="edu.neu.tiedin.ui.home.HomeFragment"
        android:label="My Trips"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_find_trip"
        android:name="edu.neu.tiedin.ui.findtrip.FindTripFragment"
        android:label="Find Trip"
        tools:layout="@layout/fragment_plan_trip" />

    <fragment
        android:id="@+id/nav_plan_trip"
        android:name="edu.neu.tiedin.ui.plantrip.PlanTripFragment"
        android:label="Plan Trip"
        tools:layout="@layout/fragment_find_trip" />
    <fragment
        android:id="@+id/nav_profile"
        android:name="edu.neu.tiedin.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/nav_settings"
        android:name="edu.neu.tiedin.ui.settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
</navigation>